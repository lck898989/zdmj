/* pages/lck/play/play.wxss */
page{
    background: #fff;
}
.mask{
  width: 750rpx;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000;
  opacity: 0.7;
  overflow: hidden;
  z-index: 1000;
}
.maskTopLevel{
  width: 750rpx;
  height: 100vh;
  position: fixed;
  top: 0;
  bottom: 0;
  background-color: #000;
  opacity: 0.7;
  overflow: hidden;
  z-index:20;
}
view{
    margin: 0;
    padding: 0;
}
.page-con{
    position: relative;
    width: 100vw;
}
.page-con-showbuy{
    height: 100vh;
    position: relative;
    width: 100vw;
}
.title-con,.text,.class-title,.class-arr,.class-item,.secondLevel-con{
    display: flex;
    flex-direction: row;
    align-items: center;
    flex-wrap: nowrap;
    justify-content: flex-start;
}
.title-con{
    position: relative;
    padding: 15rpx 18rpx;
    z-index: 25;
    background: #fff;
}
.text{
    font-size: 35rpx;
    color: #000;
    margin-left: 275rpx;
    white-space: nowrap;
}
.dot1{
    justify-content: center;
    margin-left: 5rpx;
    margin-right: 5rpx;
    margin-top: -15rpx;
}
.search-con{
    width: 210rpx;
    padding: 0rpx 20rpx;
    position: relative;
    margin-left: 80rpx;
}
.input{
    width:100rpx;
    border: 2rpx solid #9f9f9f;
    border-radius: 50rpx;
    height: 50rpx;
    font-size: 25rpx;
    padding-left: 50rpx;
    color: #9f9f9f;
}
.search-icon{
    position: absolute;
    top: 15rpx;
    left: 40rpx;
}
swiper{
    height:750rpx;
}
.swiper-con{
    position: relative;
    z-index: 25;
}
.swiper-item{
    width: 750rpx;
    height:750rpx;
}
.dots{
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: center;
    margin: 0 8rpx;
    position: absolute;
    left: 0rpx;
    right: 0rpx;
    bottom: 15rpx;
}
.dot{
    margin: 0 8rpx;
    height: 15rpx;
    width: 15rpx;
    background: #fff;
    border-radius: 15rpx;
}
.active{
    width: 40rpx;
    height: 15rpx;
    border-radius: 15rpx;
}
.scrollView{
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    flex-wrap: nowrap;
}
.twoColCon{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: flex-start;
    width: 704rpx;
}
.scrollView .twoColCon .left-con{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    flex-wrap: nowrap;
    width: 342rpx;
}
.type-con{
    width: 750rpx;
    position: relative;
    background: #fff;
}
.topLevel-con{
    position: absolute;
    top: 0;
    left: 0;
    width: 720rpx;
    background: #fff;
    padding:0rpx 15rpx;
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
    align-items: center;
    justify-content: flex-start;
    z-index: 100;
}
.class-title{
    width: 100%;
    justify-content: space-between;
    align-items: center;
    font-size: 25rpx;
}
.class-arr{
    width: 750rpx;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: center;
    margin-left: 95rpx;
}
.class-item{
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    font-size: 20rpx;
    margin-top: 30rpx;
    margin-right: 75rpx;
    margin-bottom: 30rpx;
}
.classimage-con{
    width: 100rpx;
    height: 100rpx;
    border-radius: 25rpx;
    border: 2rpx solid #e5e5e5;
    background: #eee;
}
.type-parent{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items:flex-start;
    flex-wrap: nowrap;
}
.secondLevel-con{
    width: 750rpx;
    padding: 0rpx 0rpx 25rpx 0rpx;
    flex-wrap: wrap;
    padding-left: 25rpx;
    background: #fff;
}
.secondLevel-item{
    width: 100rpx;
    height: 24rpx;
    text-align: center;
    line-height: 24rpx;
    border-radius: 15rpx;
    padding: 16rpx 30rpx;
    font-size: 25rpx;
    background: #eee;
    color: #7d7d7d;
    border: 2rpx solid #d2d2d2;
    margin-right: 17rpx;
    margin-bottom: 22rpx;
    white-space: nowrap;
}
/*
    商品种类
*/
.banner-type{
    display: flex;
    /* flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-around;
    align-items: center; */
    /*
    文字不会换行
    */
    white-space: nowrap;
    padding-left: 35rpx;
    height: 100rpx;
}
/*
隐藏横向滚动条
*/
::-webkit-scrollbar{
    width:0;
    height:0;
    color:transparent;
}
.swiperIt{
    /* width: 100rpx; */
    margin-right: 20rpx;
    font-size: 35rpx;
    padding-top: 20rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
    display: inline-block;
    text-align: center;
    color: #000;
}
.type-parent{
    justify-content: flex-start;
    width:700rpx;
}
.rightArrow{
    margin-right: 10rpx;
}
.typeactive{
    border-bottom:10rpx solid #b964d8;
    display: inline-block;
    font-weight: bold;
}
.jr-con,.jingpin,.hot,.type-con,.jr-con-p{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-wrap: nowrap;
}
.jingpin,.hot{
    justify-content: center;
    width: 350rpx;
    height: 100rpx;
    font-size: 40rpx;
    white-space: nowrap;
    border-radius: 20rpx;
    color: #fff;
    font-weight: bold;
}
.jr-con-p{
    position: relative;
    justify-content: center;
    width: 750rpx;
    height: 100rpx;
    padding: 14rpx 0rpx;
    z-index: 25;
    background: #fff;
}
.jr-con{
    height: 100rpx;
    width: 710rpx;
    
}
.jingpin:nth-child(1){
    margin-right: 0rpx;
}
/*
    三角箭头的制作
*/
.siangle{
    width:0px;
	height:0px;
	border-width:20rpx;
	border-style:solid;
	border-color:#a0a0a0 transparent transparent transparent;
    margin-bottom: -10rpx;
    margin-right: 25rpx;
}
.siangle-bottom{
    width:0px;
	height:0px;
	border-width:20rpx;
	border-style:solid;
	border-color:transparent transparent #a0a0a0 transparent;
    margin-top: -15rpx;
    margin-right: 10rpx;
}
.buy-con{
    position: fixed;
    background-color: #fff;
    width: 100%;
    height: 800rpx;
    bottom: 0rpx;
    z-index: 2000;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}
.img{
    margin-left: 20rpx;
    margin-top: 20rpx;
    border-radius: 20rpx;
}
.price-order-con{
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    flex-wrap: nowrap;
    align-items: flex-start;
    height: 160rpx;
    margin-top: 40rpx;
    margin-left: 20rpx;
}
.price-item{
    margin-top: 20rpx;
    margin-left: 15rpx;
}
.cancel-con{
    position: absolute;
    justify-self: flex-end;
    top: 10px;
    right: 20rpx;
}
.lineOne-con{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: flex-start;
}
.lineTwo-con{
    padding: 30rpx 0rpx;
    padding-bottom: 0rpx;
    margin-left: 30rpx;
}
.color-con{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    /* //允许换行 */
    flex-wrap: wrap;
    align-items: center;
}
.color-item{
    margin-top: 20rpx;
    margin-right: 20rpx;
    background-color: #eee;
    padding: 10rpx 30rpx;
    border-radius: 10rpx;
    font-size: 25rpx;
}
.choosed{
    background-color: #ec0023;
    color: #fff;
}
.add-sub-con{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    flex-wrap: nowrap;
    align-items: center;
}
.countContainer{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-wrap: nowrap;
    align-items: center;
    margin-top: 20rpx;
    font-size: 30rpx;
}
.as-item{
    margin-right: 5rpx;
    padding: 10rpx 20rpx;
}
.confirm{
    width: 750rpx;
    margin-top: 300rpx;
    font-size: 40rpx;
    height: 100rpx;
    line-height: 100rpx;
    color : #fff;
    position: fixed;
    bottom: 0rpx;
    text-align: center;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-wrap: nowrap;
    align-items: center;
}
.img-con{
    width: 750rpx;
    height: 750rpx;
    position: relative;
}
.index-num{
    /* background-color: green; */
    position: absolute;
    top: 680rpx;
    right: 20rpx;
    width: 60rpx;
    height: 26rpx;
    padding: 10rpx 25rpx;
    border: 1rpx solid #863bb7;
    border-radius: 26rpx;
    color : #863bb7;
    line-height: 26rpx;
    text-align: center;
    font-size: 25rpx;
}
.head{
    width: 678rpx;
    height:14rpx;
    line-height: 14rpx;
    padding: 36rpx;
    font-size: 28rpx;
    color: #808080;
    background-color: #f2f2f2;
}
.chooseIt{
    width: 60rpx;
    color: #808080;
    padding-left: 50rpx;
    font-size: 30rpx;
}
.addCart,.addCartn{
    color: #ec8413;
    width: 375rpx;
    font-size: 40rpx;
}
.addCartn{
    width: 350rpx;
}
.buyNow,.buyNown{
    width: 375rpx;
    background-color: #b31227;
    font-size: 40rpx;
    color: #fff;
}
.buyNown{
    width: 350rpx;
}
.addOrBuy{
    width: 100%;
    height:100rpx;
    line-height: 100rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    flex-wrap: nowrap;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    text-align: center;
}
.saveType,.saveBuy{
    width: 100%;
    background-color: #b964d8;
    color: #fff;
}
.AB{
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: nowrap;
}
.scc-con{
    width: 100%;
    height: 360rpx;
}
.goods_introduce{
    margin-bottom: 100rpx;
}
button{
    background: #fff;
}
button::after{
    border:0;
}
.share{
    width: 100rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 25rpx;
    padding: 0rpx 20rpx;
    text-align: center;
    /* background: red; */
}
.img-detail{
    position: relative;
    display: flex;
    flex-direction: column;
}
.detailImg{
    /*
        拼接的图片顶部对齐
    */
    vertical-align: top;
}
.goTop{
    position: fixed;
    bottom: 50rpx;
    padding: 20rpx;
    right: 30rpx;
    z-index: 1000;
}
.wx-swiper-dots.wx-swiper-dots-horizontal{
    margin-bottom: 0rpx;
    float: left;
    text-align: center;
}
.wx-swiper-dot::before{
    content: '';
    flex-grow: 1;
    background: #fff;
    border-radius: 15rpx;
    border: 0rpx;
}
.wx-swiper-dot.wx-swiper-dot{
    width: 15rpx;
    height: 15rpx;
    border-radius: 15rpx;
    background: #fff;
}
.wx-swiper-dot.wx-swiper-dot-active{
    width: 40rpx;
    height: 15rpx;
    border-radius: 15rpx;
    background: #fff;
}
.secondLevelActive{
    border: 2rpx solid #863bb7;
    background: #ecceff;
    color: #b25bd2;
}
.loadText{
    width: 750rpx;
    height: 50rpx;
    position: absolute;
    font-size: 25rpx;
    color: #000;
    bottom: -35rpx;
    left: 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    flex-wrap: nowrap;
}
.centerText{
    /* width:  */
    width: 700rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
}
.emptyGoodsCon{
    height: 300rpx;
    background: #fff;
    width: 100%;
}
