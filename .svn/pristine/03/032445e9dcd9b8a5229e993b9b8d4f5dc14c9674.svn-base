/* pages/lck/applyProgress/applyProgress.wxss */
page{
    background:#f2f2f2;
}
.page-con{
    padding-bottom: 140rpx;
}
.applyGoods{
    /* margin-top: 20rpx; */
    margin-bottom: 4rpx;
}
.process-con{
    height: 36rpx;
    line-height: 36rpx;
    background: #fff;
    padding: 35rpx 20rpx 35rpx 42rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-flow: nowrap;
    align-items: center;
    color: #404040;
}
.process-detail{
    display: flex;
    align-items: center;
    flex-direction: row;
}
.process-detail{
    background: #fff;
    margin-top: 5rpx;
    padding: 25rpx 0rpx;
}
.process-leftd{
    font-size: 30rpx;
}
.process-right{
    width:750rpx;
}
.detail-item:last-child{
    padding-bottom: 0rpx;
}
.program-con{
    background: #fff;
    margin-top: 10rpx;
    padding: 35rpx 0rpx;
}
.top-text{
    width: 700rpx;
    margin: 0rpx auto;
    padding: 35rpx 0rpx 20rpx 0rpx;
    border-bottom: 2rpx solid #f2f2f2;
    font-size: 30rpx;
}
.bottom-text{
    width: 700rpx;
    margin: 0rpx auto;
    padding: 35rpx 0rpx 35rpx 25rpx;
    color: #c7c7c7;
    font-size: 25rpx;
}
.server-order{
    margin-top:15rpx; 
    background: #fff;
}
.delete-apply{
    width: 750rpx;
    position: fixed;
    bottom: 0rpx;
    height: 115rpx;
    background: #fff;
    margin-top: 25rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}
.applyDelete{
    font-weight: 500;
    margin-right: 20rpx;
    width: 140rpx;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 25rpx;
    border: 2rpx solid #525252;
    border-radius: 10rpx;
    text-align: center;
}
.order{
    font-size: 30rpx;
    position: relative;
    /* padding-bottom: 5rpx; */
}
/* .order:last-child{
    border-bottom: 0rpx solid #fff;
} */
.order-item{
    margin-bottom: 25rpx;
}
.icon{
    width: 15rpx;
    height: 15rpx;
    border-radius: 50%;
    background: #c3c3c3;
    margin-left: 40rpx;
    margin-right: 40rpx;
}
.logistics_centent{
    width: 600rpx;
    position: absolute;
    top: -2rpx;
    left: 100rpx;
    font-size: 28rpx;
    color: #666;
    border-bottom: 2rpx solid #f2f2f2;
}
.logistics_centent:last-child{
    top: 5rpx;
}
.in{
    border: 2rpx solid #f65c60;
    background: #f65c60;
    width: 28rpx;
    height:28rpx;
    margin-left: 35rpx;
}
.no_in{
    border : 2rpx solid #d0d0d0;
}
.on{
    background: #d0d0d0;
    height: 180rpx;
    width: 2rpx;
    margin-left: 48rpx;
}
.right-text{
    width: 600rpx;
    border-bottom: 2rpx solid #f2f2f2;
    padding-bottom: 25rpx;
}
.server-value{
    color: #404040; 
}
