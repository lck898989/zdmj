<view>
    <view bindcustomevent='eventListener'>
    </view>
    <view class='list-con' bindtap='onTap'>
        <view class='img-con'>
            <image src="{{host}}{{goods.head}}" style="width:350rpx;height:150px;"></image>
        </view>
        <view class='text-con'>
            <view class='top-text'>
                <text>{{goods.description}}</text>
            </view>
            <view class='bottom-text'>
                <view style='font-size:30rpx;'>
                    供应商地址(天津)
                </view>
                <view class='price-con'>
                    <text style='fone-size:30rpx;'>￥{{goods.price}}</text>
                </view>
            </view>
            <view class='detail-con'>
                <view class='detail-left'>
                    <view style='font-size:30rpx;'>
                        已有10人购买                    
                    </view>
                    <view style='font-size:30rpx;'>
                        可赚返利10.8元
                    </view>
                </view>
                <view class='detail-right' style='font-size:30rpx;'>
                    商品详情
                </view>
            </view>
            <!--需要动态添加的元素在这里添加-->
            <slot name='text-extra'></slot>
        </view>
        <slot name='flex-row-last'></slot>
    </view>
</view>
